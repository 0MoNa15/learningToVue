;;AAEA,eAAA,QAAA;AACA,OAbA,CAAA;AAcA;AAvBA,GALA;AA+BA,EAAA,IA/BA,kBA+BA;AACA,WAAA;AACA,MAAA,MAAA,EAAA,EADA;AAEA,MAAA,SAAA,EAAA;AAFA,KAAA;AAIA,GApCA;AAsCA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA;AACA,MAAA,IAAA,EAAA,KADA;AAEA,MAAA,OAAA,EAAA;AAAA,eAAA,EAAA;AAAA;AAFA;AADA,GAtCA;AA6CA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,oBACA,EADA,EACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,aAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAAA,OAAA;AACA,KAHA;AAKA,IAAA,eALA,6BAKA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,sBAAA;AACA,WAAA,SAAA,GAAA,KAAA,SAAA,IAAA,CAAA,GAAA,CAAA,CAAA,GAAA,CAAA;AACA;AARA;AA7CA,CAAA","sourcesContent":["<template>\n  <table>\n    <thead>\n      <tr class=\"bg-gray-100 border-b-2 border-gray-400\">\n        <th></th>\n        <th :class=\"{ up: this.sortOrder == 1, down: this.sortOrder == -1 }\">\n          <span @click=\"changeSortOrder\" class=\"underline cursor-pointer\">\n            Ranking\n          </span>\n        </th>\n        <th>Nombre</th>\n        <th>Precio</th>\n        <th>Cap. de Mercado</th>\n        <th>VariaciÃ³n 24hs</th>\n        <td class=\"hidden sm:block\">\n          <input\n            class=\"bg-gray-100 focus:outline-none border-b border-gray-400 py-2 px-4 block\"\n            id=\"filter\"\n            placeholder=\"Buscar\"\n            type=\"text\"\n            v-model=\"filter\"\n          />\n        </td>\n      </tr>\n    </thead>\n    <tbody>\n      <tr\n        v-for=\"a in filteredAssets\"\n        class=\"border-b border-gray-200 hover:bg-gray-100 hover:bg-orange-100\"\n        :key=\"a.id\"\n      >\n        <td>\n          <img\n            class=\"w-6 h-6\"\n            :src=\"\n              `https://static.coincap.io/assets/icons/${a.symbol.toLowerCase()}@2x.png`\n            \"\n            alt=\"a.name\"\n          />\n        </td>\n        <td>\n          <b> #{{ a.rank }} </b>\n        </td>\n        <td>\n          <router-link\n            class=\"hover:underline text-green-600\"\n            :to=\"{ name: 'coin-detail', params: { id: a.id } }\"\n          >\n            {{ a.name }}\n          </router-link>\n\n          <small class=\"ml-1 text-gray-500\">\n            {{ a.symbol }}\n          </small>\n        </td>\n        <td>{{ a.priceUsd | dollar }}</td>\n        <td>{{ a.marketCapUsd | dollar }}</td>\n        <td\n          :class=\"\n            a.changePercent24Hr.includes('-')\n              ? 'text-red-600'\n              : 'text-green-600'\n          \"\n        >\n          {{ a.changePercent24Hr | percent }}\n        </td>\n        <td class=\"hidden sm:block\">\n          <px-button @custom-click=\"goToCoin(a.id)\">\n            <span>Detalle</span>\n          </px-button>\n        </td>\n      </tr>\n    </tbody>\n  </table>\n</template>\n\n<script>\nimport PxButton from '@/components/PxButton'\nexport default {\n  name: 'PxAssetsTable',\n\n  components: { PxButton },\n\n  computed: {\n    filteredAssets() {\n      /*if (!this.filter) {\n        return this.assets\n      }*/\n      console.log('TEST filteredAssets')\n\n      const altOrder = this.sortOrder == 1 ? -1 : 1\n\n      return this.assets\n        .filter(\n          a =>\n            a.symbol.toLowerCase().includes(this.filter.toLowerCase()) ||\n            a.name.toLowerCase().includes(this.filter.toLowerCase())\n        )\n        .sort((a, b) => {\n          if (parseInt(a.rank) > parseInt(b.rank)) {\n            console.log('TEST sort')\n            return this.sortOrder\n          }\n\n          return altOrder\n        })\n    }\n  },\n\n  data() {\n    return {\n      filter: '',\n      sortOrder: 1\n    }\n  },\n\n  props: {\n    assets: {\n      type: Array,\n      default: () => []\n    }\n  },\n\n  methods: {\n    goToCoin(id) {\n      this.$router.push({ name: 'coin-detail', params: { id } })\n    },\n\n    changeSortOrder() {\n      console.log('TEST changeSortOrder')\n      this.sortOrder = this.sortOrder == 1 ? -1 : 1\n    }\n  }\n}\n</script>\n\n<style scoped>\n.up::before {\n  content: 'ðŸ‘†';\n}\n\n.down::before {\n  content: 'ðŸ‘‡';\n}\n\ntd {\n  padding: 20px 0px;\n  font-size: 0.6rem;\n  text-align: center;\n}\n\nth {\n  padding: 5px;\n  font-size: 0.6rem;\n}\n\n@media (min-width: 640px) {\n  td,\n  th {\n    padding: 20px;\n    font-size: 1rem;\n  }\n\n  th {\n    padding: 12px;\n  }\n}\n</style>\n"],"sourceRoot":"src/components"}]}